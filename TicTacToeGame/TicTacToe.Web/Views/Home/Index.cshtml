@using TicTacToe.Web.FrameworkExtentions.UserIdentityHelpers

@{
    ViewBag.Title = "Home Page";
}

@if (UserIdentityHelper.UserIsLoggedIn(this.User))
{
    RenderAuthenticatedUserIndexView();
}
else
{
    RenderAnonymousUserIndexView();
}

@functions {
    public void RenderAuthenticatedUserIndexView()
    {
        Html.RenderPartial("_AuthenticatedUserIndex");
    }

    public void RenderAnonymousUserIndexView()
    {
        Html.RenderPartial("_AnonymousUserIndex");
    }
}