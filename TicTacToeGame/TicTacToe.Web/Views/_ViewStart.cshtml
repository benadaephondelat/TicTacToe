@using TicTacToe.Web.Views.ViewConstants;
@using TicTacToe.Web.FrameworkExtentions.UserIdentityHelpers;

@{
    if (UserIdentityHelper.UserIsLoggedIn(this.User))
    {
        Layout = ViewConstants.AuthenticatedUserLayout;
    }
    else
    {
        Layout = ViewConstants.AnonymousUserLayout;
    }
}